<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javbus.spider.spider.dao.relation.MovieBigImageDao">
    
    <insert id="addMovieBigImageRelation" parameterType="com.javbus.spider.spider.entity.relation.MovieDirectorRelation">
        INSERT INTO movie_bigImage_relation (movie_id, big_image_id)
        VALUES (#{movieId}, #{bigImageId})
    </insert>

    <delete id="deleteMovieBigImageRelation">
        DELETE FROM movie_bigImage_relation
        WHERE movie_id = #{movieId} AND big_image_id = #{bigImageId}
    </delete>
</mapper>