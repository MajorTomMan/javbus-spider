<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javbus.spider.spider.dao.relation.MovieStudioDao">
        <!-- 添加电影与工作室关系 -->
        <insert id="addMovieStudioRelation" parameterType="com.javbus.spider.spider.entity.relation.MovieStudioRelation">
            INSERT INTO movie_studio_relation (movie_id, studio_id)
            VALUES (#{movieId}, #{studioId})
        </insert>
    
        <!-- 删除电影与工作室关系 -->
        <delete id="deleteMovieStudioRelation">
            DELETE FROM movie_studio_relation
            WHERE movie_id = #{movieId} AND studio_id = #{studioId}
        </delete>
</mapper>