<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.javbus.spider.spider.dao.relation.StarSeriesDao">

    <insert id="addStarSeriesRelation" parameterType="com.javbus.spider.spider.entity.relation.StarSeriesRelation">
        INSERT INTO star_series_relation (star_id, series_id)
        VALUES (#{starId}, #{seriesId})
    </insert>

    <delete id="deleteStarSeriesRelation">
        DELETE FROM star_series_relation
        WHERE star_id = #{starId} AND series_id = #{seriesId}
    </delete>

    <!-- Other operations as needed -->

</mapper>
